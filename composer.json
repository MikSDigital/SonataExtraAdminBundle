{
    "name": "picoss/sonata-extra-admin-bundle",
    "type": "symfony-bundle",
    "description": "Sonata Extra admin features",
    "keywords": ["admin","sonata"],
    "homepage": "http://github.com/picoss/SonataExtraAdminBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Honel",
            "email": "romain.honel@gmail.com"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0",
        "sonata-project/admin-bundle": "^3.1",
        "sonata-project/core-bundle": "^3.0",
        "symfony/framework-bundle": "^2.2 || ^3.0"
    },
    "require-dev": {
        "gedmo/doctrine-extensions": ">=2.2"
    },
    "suggest": {
        "stof/doctrine-extensions-bundle": "1.1.*@dev"
    },
    "autoload": {
        "psr-0": { "Picoss\\SonataExtraAdminBundle": "" }
    },
    "target-dir": "Picoss/SonataExtraAdminBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
